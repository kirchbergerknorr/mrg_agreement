<config>
	<modules>
		<Symmetrics_Agreement>
			<version>0.2.5</version>
		</Symmetrics_Agreement>
	</modules>
	<global>
		<models>
			<checkout>
				<rewrite>
					<agreement>Symmetrics_Agreement_Model_Agreement</agreement>
				</rewrite>
			</checkout>
		</models>
        <resources>
            <agreement_setup>
                <setup>
                    <module>Symmetrics_Agreement</module>
                    <class>Symmetrics_Agreement_Model_Setup</class>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </agreement_setup>
            <agreement_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </agreement_write>
            <agreement_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </agreement_read>
        </resources>
    </global>
</config>
