<?xml version="1.0"?>
<!--
/**
 * @category    Symmetrics
 * @package     Symmetrics_Agreement
 * @author      symmetrics gmbh <info@symmetrics.de>, Eugen Gitin <eg@symmetrics.de>
 */
 -->
<config>
	<modules>
		<Symmetrics_Agreement>
			<version>0.2.1</version>
			<depends />
		</Symmetrics_Agreement>
	</modules>
	<global>
		<models>
			<checkout>
				<rewrite>
					<agreement>Symmetrics_Agreement_Model_Agreement</agreement>
				</rewrite>
			</checkout>
		</models>
		<blocks>
            <checkout>
                <rewrite>
                    <agreements>Symmetrics_Agreement_Block_Agreement</agreements>
                </rewrite>
            </checkout>
		</blocks>
        <resources>
            <agreement_setup>
                <setup>
                    <module>Symmetrics_Agreement</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </agreement_setup>
            <agreement_setup>
                <connection>
                    <use>core_write</use>
                </connection>
            </agreement_setup>
            <agreement_setup>
                <connection>
                    <use>core_read</use>
                </connection>
            </agreement_setup>
        </resources>
    </global>
</config>
